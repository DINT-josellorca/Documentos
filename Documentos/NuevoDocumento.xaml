<Window x:Class="Documentos.NuevoDocumento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Documentos"
        mc:Ignorable="d"
        x:Name="NuevoDocumentoWindow"
        Title="NuevoDocumento" Height="450" Width="800" Closing="NuevoDocumentoWindow_Closing">
    <DockPanel> 
        <Button DockPanel.Dock="Bottom" x:Name="GuardarButton" Content="Guardar" Margin="10" Click="GuardarButton_Click"></Button>
        <TextBox x:Name="DocumentoTextBox" AcceptsReturn="True"></TextBox>
    </DockPanel>
</Window>
